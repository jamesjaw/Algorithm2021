#include <iostream>
using namespace std;

int cmp(const void*a, const void*b){
    return *(int*)a < *(int*)b;
}
int main(){
    std::ios_base::sync_with_stdio(false);
    std::cin.tie(nullptr);
    
    int n;
    cin>>n;
    int* ar = new int[n];
    for(int i=0;i<n;i++) cin>>ar[i];
    qsort(ar,n,sizeof(int),cmp);
    int z = (n-1)/2;
    
    cout<<ar[z]<<"\n";
    return 0;
}
